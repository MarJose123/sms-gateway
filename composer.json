{
    "name": "marjose123/sms-gateway",
    "description": "This is my package sms-gateway",
    "keywords": [
        "MarJose123",
        "laravel",
        "sms-gateway"
    ],
    "homepage": "https://github.com/marjose123/sms-gateway",
    "license": "MIT",
    "authors": [
        {
            "name": "MarJose123",
            "email": "18107626+MarJose123@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0||^10.0",
        "laravel/sanctum": "^3.3||^4.0.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "MarJose123\\SmsGateway\\": "src/",
            "MarJose123\\SmsGateway\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarJose123\\SmsGateway\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-sms-gateway --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarJose123\\SmsGateway\\SmsGatewayServiceProvider",
                "MarJose123\\SmsGateway\\Providers\\RepositoryServiceProvider"
            ],
            "aliases": {
                "SmsGateway": "MarJose123\\SmsGateway\\Facades\\SmsGateway"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
